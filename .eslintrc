{
    "env": {
        "browser": true
    },
    "extends": ["airbnb-base"],
    "plugins": ["html"],
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "no-unused-expressions": 0,
        "no-param-reassign": 0,
        "comma-dangle": 0,
        "semi": 0,
        "arrow-parens": 0
    }
}
